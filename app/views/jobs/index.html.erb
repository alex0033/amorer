<div class="main-contents col-md-8">
  <%= form_tag(jobs_path, method: :get, class: "search-form") do %>
    <%= text_field_tag :search %>
    <%= submit_tag '検索', name: nil, class: "search-btn" %>
  <% end %>
  <div class="jobs-box my-5">
    <%= will_paginate %>
    <div class="jobs">
      <%= render @jobs %>
    </div>
    <%= will_paginate %>
  </div>
</div>
